{
  "name": "todo",
  "version": "0.1.0",
  "description": "A clone implementation of todoist app",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:app\"",
    "dev:app": "npm run dev -w packages/app",
    "dev:api": "npm run dev -w packages/api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InfantAjayVenus/todo.git"
  },
  "keywords": [
    "react",
    "react-router-dom",
    "express",
    "typescript",
    "npm-workspaces",
    "docker",
    "todoist-clone"
  ],
  "author": "Infant Ajay Venus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/InfantAjayVenus/todo/issues"
  },
  "homepage": "https://github.com/InfantAjayVenus/todo#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
